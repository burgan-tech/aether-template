{
  "ConnectionStrings": {
    "Default": "Host=localhost,5432;Database=AetherMyProjectNameDb;Username=postgres;Password=postgres"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ResponseCompression": {
    "Enable": true,
    "Providers": ["gzip"],
    "EnableForHttps": true,
    "MimeTypes": [],
    "ExcludedMimeTypes": []
  },
  "Redis": {
    "Mode": "Standalone",
    "InstanceName": "myprojectname-api",
    "ConnectionTimeout": 5000,
    "DefaultDatabase": 0,
    "Password": "",
    "Ssl": false,
    "Standalone": {
      "EndPoints": ["localhost:6378"]
    },
    "Cluster": {
      "EndPoints": ["localhost:7000", "localhost:7001", "localhost:7002"],
      "MaxRedirects": 3
    },
    "Sentinel": {
      "Masters": ["mymaster"],
      "Sentinels": ["localhost:26379", "localhost:26380"],
      "DefaultDatabase": 0
    },
    "RetryPolicy": {
      "MaxRetries": 3,
      "RetryTimeout": 1000
    }
  },
  "Telemetry": {
    "ServiceName": "MyProjectName",
    "ServiceVersion": "1.0.0",
    "Environment": "Development",
    "TraceProvider": "zipkin",
    "LogProvider": "file",
    "Logging": {
      "Enabled": true,
      "FilePath": "Logs/myprojectname-api.log",
      "MinimumLevel": "Information",
      "Enrichers": {
        "Headers": [
          "x-custom-app-name",
          "x-correlation-id",
          "x-request-id"
        ]
      }
    },
    "Zipkin": {
      "Endpoint": "http://localhost:9411/api/v2/spans"
    }
  }
}
